{
	"name": "queryr/term-store",
	"type": "library",
	"description": "Persistence of Wikibase terms with lookups of those terms and by those terms",
	"keywords": [
		"Wikibase",
		"DataModel",
		"Terms",
		"Fingerprint",
		"Lookup"
	],
	"homepage": "https://github.com/JeroenDeDauw/TermStore",
	"license": "GPL-2.0+",
	"authors": [
		{
			"name": "Jeroen De Dauw",
			"email": "jeroendedauw@gmail.com",
			"homepage": "http://jeroendedauw.com"
		}
	],
	"require": {
		"php": ">=5.5.0",

		"wikibase/data-model": "~4.3|~3.0|~2.5",
		"doctrine/dbal": "~2.4"
	},
	"require-dev": {
		"ext-sqlite3": "*",
		"whatthejeff/nyancat-phpunit-resultprinter": "~1.2"
	},
	"autoload": {
		"psr-4": {
			"Queryr\\TermStore\\": "src/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "0.2.x-dev"
		}
	}
}
